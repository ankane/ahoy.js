{
  "extension": [
    ".js",
    ".cjs",
    ".mjs"
  ],
  "include": [
    "*"
  ],
  "exclude": [
    "coverage",
    "test"
  ],
  "reporter": [
    "text",
    "html",
    "json",
    "lcov"
  ],
  "all": false,
  "check-coverage": false,
  "statements": 100,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "sourceMap": true,
  "instrument": true
}
